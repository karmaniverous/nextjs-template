{
  "name": "@karmaniverous/next-template",
  "description": "Next.js ES6 template with support for Redux Toolkit, Serify, Lodash, Semantic UI, and Mocha + Chai.",
  "author": "@karmaniverous",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@karmaniverous/merge-delimited-lists": "^0.0.2",
    "@karmaniverous/serify-deserify": "^1.2.9",
    "@reduxjs/toolkit": "^1.9.1",
    "@tsconfig/recommended": "^1.0.1",
    "axios": "^1.2.1",
    "lodash": "^4.17.21",
    "next": "^12.2.2",
    "next-auth": "^4.18.4",
    "next-redux-wrapper": "~7.0.5",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-json-pretty": "^2.2.0",
    "react-redux": "^8.0.5",
    "react-scroll": "^1.8.8",
    "semantic-ui-less": "^2.5.0",
    "semantic-ui-react": "^2.1.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.5",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-syntax-import-assertions": "^7.20.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/register": "^7.18.9",
    "@next/bundle-analyzer": "^12.3.1",
    "@next/eslint-plugin-next": "^12.2.2",
    "@types/node": "^18.11.11",
    "@types/react": "^18.0.26",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-extension": "^0.1.3",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "chai-match-pattern": "^1.3.0",
    "commander": "^9.4.1",
    "cross-env": "^7.0.3",
    "directory-tree": "^3.4.0",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.29.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-mocha": "^10.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.11",
    "fs-extra": "^11.1.0",
    "mocha": "^10.1.0",
    "next-with-less": "^2.0.5",
    "release-it": "^15.6.0",
    "treeify": "^1.1.0"
  },
  "main": "index.js",
  "mocha": {
    "require": [
      "@babel/register",
      "chai-as-promised",
      "chai-match-pattern"
    ],
    "spec": "./{,!(node_modules)/**}/*.test.*js*"
  },
  "prettier": {
    "useTabs": false,
    "singleQuote": true,
    "proseWrap": "always",
    "tabWidth": 2
  },
  "release-it": {
    "github": {
      "release": true
    },
    "npm": {
      "publish": false
    }
  },
  "scripts": {
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "build": "next build",
    "cli": "node ./cli/cli.mjs",
    "dev": "next dev",
    "release": "npx dotenv -c -- release-it",
    "start": "next start",
    "test": "mocha"
  },
  "version": "0.1.0"
}
